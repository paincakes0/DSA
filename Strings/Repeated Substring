class Solution {
public:
    bool repeatedSubstringPattern(string s) {
        int l = s.length();
        for(int n = l/2; n>0; n--){
            if(l%n==0){
                int times = l/n;
                string pattern = s.substr(0,n);
                string newStr = "";
                while(times--){
                    newStr += pattern;
                }
                if(newStr == s) return true;
            }
        }
        return false;
    }
};
